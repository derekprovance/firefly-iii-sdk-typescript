{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "esModuleInterop": true,
        "inlineSourceMap": false,
        "lib": [
            "esnext"
        ],
        "listEmittedFiles": false,
        "listFiles": false,
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "pretty": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "traceResolution": false,
        "types": [],
        "module": "commonjs",
        "outDir": "dist",
        "target": "es2015"
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules",
        "dist"
    ]
}